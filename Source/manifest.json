{
	"manifest_version": 2,

	"name": "Bears to Backpakers",
    "short_name": "Backpakers",
	"description": "Learn about Backpakers",
	"author": "Elizabeth Simonian",
	"version": "1.0",

	"icons": {
		"16": "icon16.png",
		"32": "icon32.png",
		"48": "icon48.png",
		"128": "icon128.png"
	},

	"content_scripts":
	[
		{
			"matches": ["*://*/*"],
			"all_frames": true,
			"js": ["content_script.js"],
			"run_at": "document_end"
		}
	]
}
